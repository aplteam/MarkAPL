 expected,←'<tfoot>' '<tr>' '<td style="text-align: left;">Total</td>' '<td style="text-align: right;">10</td>' '</tr>' '</tfoot>'
 expected,←⊂'</table>'

 →PassesIf expected≡ns.html

 parms←##.MarkAPL.CreateParms
 parms.markdownStrict←1
 ns←##.MarkAPL.Init parms md
 ns←##.MarkAPL.Process ns

 expected←⊂'<table>'
 expected,←'<thead>' '<tr>' '<th>Col 1</th>' '<th style="text-align: right;">Col 2</th>' '</tr>' '</thead>'
 expected,←'<tbody>' '<tr>' '<td>ABC</td>' '<td style="text-align: right;">10</td>' '</tr>' '<tr>' '<td>=====</td>' '<td style="text-align: right;">=====</td>' '</tr>' '<tr>' '<td>Total</td>' '<td style="text-align: right;">10</td>' '</tr>' '</tbody>'
